package sample11;

public class sample11 {


	

	public static void main(String[] args) {
		// TODO Auto-generated method stub
			
			double dum = 160.5;
			
			System.out.println("身高是"+dum+"公分");
			System.out.println("指定給int型態的變數");
			
			int inum = (int) dum;
			System.out.println("身高是"+inum+"公分");
	}

}
